@model List<ead_frontend.Models.HighUseChart>
@{
    ViewBag.Title = "High Use Components";
    Layout = "../Shared/_RequestLayout.cshtml";
}

<h2>High Use Components</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table table-striped table-small">
        <thead>
        <tr>
            <th>EADID</th>
            <th>Component</th>
            <th>Times Requested</th>
        </tr>
        </thead>
        @foreach (var collection in Model)
        {
            <tr>
                <td>@collection.identifier</td>
                <td>@collection.box</td>
                <td>@collection.count</td>
            </tr>
        }

    </table>
}