<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="CookieDomain" value=".ecu.edu" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="LoginUrl" value="http://lib.ecu.edu/central/Login/digital?ReturnUrl=" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="LogoutUrl" value="http://lib.ecu.edu/central/logout" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>
